#pragma hdrstop
#pragma argsused

#ifdef _WIN32
#include <tchar.h>
#else
  typedef char _TCHAR;
  #define _tmain main
#endif

#include "rs232.h"
#include <stdio.h>
#include <conio.h>
#include <time.h>

int _tmain(int argc, _TCHAR* argv[]) 
{
	rs232 liaison;
	liaison.Ouvrirport();
	liaison.ecrireport();

	liaison.lireport();


	getch();
	return 0;
}
